<a:For list="$^categoryTestUnits" item="$currentTestUnit">
    <table class="testUnit">
        <tr>
            <td class="tableHeader">$currentTestUnit.mainName ($currentTestUnit.secondaryName)</td>
            <td class="tableHeader">Context</td>
        </tr>

        <tr>
            <td class="tableCell">
                <div class="leftLinks">
                    <a:If ifTrue="$currentTestUnit.hasUiNoParamLinks">
                        <ul class="testLinkList">
                            <a:For list="$currentTestUnit.uiNoParamLinks" item="$currentTestUnitLink">
                                <a:If ifTrue="$currentTestUnitLinkActive">
                                    <li class="appLink">
                                        <a:Hyperlink action="$testUnitClick" title="$currentTestUnitLink.secondaryName">
                                            $currentTestUnitLink.displayName
                                        </a:Hyperlink>
                                    </li>
                                </a:If>
                                <a:Else>
                                    <li class="appLink">
                                        $currentTestUnitLink.displayName
                                    </li>
                                </a:Else>
                            </a:For>
                        </ul>
                    </a:If>
                    <a:If ifTrue="$currentTestUnit.hasUiNoParamLinks">
                        <a:If ifTrue="$currentTestUnit.hasNoUiNoParamLinks">
                            <div class="separator"></div>
                        </a:If>
                    </a:If>
                    <a:If ifTrue="$currentTestUnit.hasNoUiNoParamLinks">
                        <ul class="testLinkList">
                            <a:For list="$currentTestUnit.noUiNoParamLinks" item="$currentTestUnitLink">
                                <a:If ifTrue="$currentTestUnitLinkActive">
                                    <li class="reloadLink">
                                        <a:Hyperlink action="$testUnitClick" title="$currentTestUnitLink.secondaryName">
                                            $currentTestUnitLink.displayName
                                        </a:Hyperlink>
                                    </li>
                                </a:If>
                                <a:Else>
                                    <li class="appLink">
                                        $currentTestUnitLink.displayName
                                    </li>
                                </a:Else>
                            </a:For>
                        </ul>
                    </a:If>
                </div>
            </td>
            <td class="tableCell">
                <div class="rightLinks">
                    <a:If ifTrue="$currentTestUnit.hasUiParamLinks">
                        <ul class="testLinkList">
                            <a:For list="$currentTestUnit.uiParamLinks" item="$currentTestUnitLink">
                                <a:If ifTrue="$currentTestUnitLinkActive">
                                    <li class="appLink">
                                        <a:Hyperlink action="$testUnitClick" title="$currentTestUnitLink.secondaryName">
                                            $currentTestUnitLink.displayName
                                        </a:Hyperlink>
                                    </li>
                                <a:Else/>
                                    <li class="appLink">
                                        $currentTestUnitLink.displayName
                                    </li>
                                </a:If>
                            </a:For>
                        </ul>
                    </a:If>
                    <a:If ifTrue="$currentTestUnit.hasUiParamLinks">
                        <a:If ifTrue="$currentTestUnit.hasNoUiParamLinks">
                            <div class="separator"></div>
                        </a:If>
                    </a:If>
                    <a:If ifTrue="$currentTestUnit.hasNoUiParamLinks">
                        <ul class="testLinkList">
                            <a:For list="$currentTestUnit.noUiParamLinks" item="$currentTestUnitLink">
                                <a:If ifTrue="$currentTestUnitLinkActive">
                                    <li class="reloadLink">
                                        <a:Hyperlink action="$testUnitClick" title="$currentTestUnitLink.secondaryName">
                                            $currentTestUnitLink.displayName
                                        </a:Hyperlink>
                                    </li>
                                <a:Else/>
                                    <li class="appLink">
                                        $currentTestUnitLink.displayName
                                    </li>
                                </a:If>
                            </a:For>
                        </ul>
                    </a:If>
                </div>
            </td>
        </tr>
    </table>
    &nbsp;
</a:For>